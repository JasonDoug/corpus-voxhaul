{
  "name": "pdf-lecture-service",
  "version": "1.0.0",
  "description": "Transform scientific PDFs into engaging audio lectures with synchronized playback",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/local-server/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "bundle": "esbuild src/functions/**/*.ts --bundle --platform=node --target=node20 --outdir=dist/functions"
  },
  "keywords": [
    "pdf",
    "lecture",
    "audio",
    "serverless",
    "education"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/pdf-parse": "^1.1.5",
    "aws-sdk": "^2.1498.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pdf-parse": "^2.2.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "esbuild": "^0.19.10",
    "eslint": "^8.56.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
